{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "Request",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "Response"
    },
    {
      "name": "QueueItem",
      "type": "queue",
      "direction": "out",
      "queueName": "%queueName%",
      "connection": "AzureWebJobsStorage"
    }
  ]
}